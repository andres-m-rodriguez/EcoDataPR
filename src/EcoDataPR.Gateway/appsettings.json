{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ReverseProxy": {
    "Routes": {
      "aquatrack": {
        "ClusterId": "aquatrack",
        "Match": {
          "Path": "{**catch-all}"
        }
      }
    },
    "Clusters": {
      "aquatrack": {
        "Destinations": {
          "destination1": {
            "Address": "http://aquatrack-webapp"
          }
        }
      }
    }
  }
}
